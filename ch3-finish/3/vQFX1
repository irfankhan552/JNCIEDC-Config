protocols {
    bgp {
        group pod-leaf {
            hold-time 9;
            authentication-key "$9$lo3KWxJZjmPQDiIcylLXUjHmT3"; ## SECRET-DATA
            export [ advertise-lo0 advertise-direct ];
            peer-as 65500;
            multipath;
            neighbor 192.168.1.1;
            neighbor 192.168.2.1;
        }
        group overlay {
            type internal;
            local-address 172.16.1.1;
            family evpn {
