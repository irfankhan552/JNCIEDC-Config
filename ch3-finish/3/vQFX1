policy-options {
    prefix-list fabric-direct-xe-0/0/2 {
        apply-path "interfaces xe-0/0/2 unit <*> family inet address <*>";
    }
    policy-statement advertise-direct {
        term xe-0/0/2 {
            from {
                protocol direct;
                prefix-list fabric-direct-xe-0/0/2;
            }
            then {
                community add vqfx1;
                accept;
            }
        }
    }
    policy-statement advertise-lo0 {
        term lo0 {
            from {
                protocol direct;
                route-filter 172.16.1.0/24 prefix-length-range /32-/32;
            }
            then {
                community add vqfx1;
                accept;
            }
        }
    }
    policy-statement fabric-import {
        term fabric-l3 {
            from community fabric-l3;
            then accept;
        }
    }
    policy-statement lbpp {
        term lbpp {
            then {
                load-balance per-packet;
            }
        }
    }
    community fabric-l3 members target:65534:1;
    community vqfx1 members 65000:1;
    community vqfx2 members 65000:2;
    community vqfx3 members 65000:3;
}
switch-options {
    vtep-source-interface lo0.0;
    route-distinguisher 172.16.1.1:65000;
    vrf-import fabric-import;
    vrf-target {
        target:1:1;
        auto;
    }
}
